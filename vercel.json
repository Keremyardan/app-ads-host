{
  "routes": [
    {
      "src": "/app-ads.txt",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "dest": "/app-ads.txt"
    },
    {
      "src": "/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://www.hushuapp.com/$1"
      }
    }
  ]
}
